
public class sand2 {
	public static void main(String[] args){
		int[] myArray = {10, 18, 4, 24, 33, 40, 8, 3, 12};
		insertionSort(myArray);
		System.out.println(myArray.toString());
	}
	
	public static void insertionSort(int[] arr) {
		for (int i = 1; i < 5; i++) {
			if (arr[i] < arr[i-1]) {
				int toInsert = arr[i];
				int j = i;
				do {
					arr[j] = arr[j-1];
					j = j - 1;
				} while (j > 0 && toInsert < arr[j-1]);
				arr[j] = toInsert;
			}
		}
	}
}
