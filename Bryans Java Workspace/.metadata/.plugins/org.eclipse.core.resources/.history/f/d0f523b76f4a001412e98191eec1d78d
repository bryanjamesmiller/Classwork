
public class sandbag1 
{
	public static void main (String [] args)
	{
		generateSums(5);
	}

	public static void generateSums(int n) {
		for (int i = 1; i <= n; i++) {
			int sum = 0;
			for (int j = 1; j <= i; j++) {
				sum = sum + j; // how many times is this executed?
			}
			System.out.println(sum);
		}
	}
}
